<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <OutputType>Library</OutputType>
    <RootNamespace>DMXForGamers.Web</RootNamespace>
    <AssemblyName>DMXForGamers.Web</AssemblyName>
