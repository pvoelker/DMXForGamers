  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\DMXForGamers.Models\DMXForGamers.Models.csproj" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Views\Codes\403.cshtml" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Views\Main\Index.cshtml" />
  </ItemGroup>
  <ItemGroup>
